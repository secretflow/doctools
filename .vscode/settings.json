{
  "[css][javascript][javascriptreact][json][jsonc][typescript][typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.fixAll.stylelint": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": true,
      "other": true,
      "strings": true
    }
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
  },
  "files.associations": {
    "**/tooling/tsconfig/*.json": "jsonc",
    "**/tooling/tsconfig/package.json": "json",
    "nx.json": "jsonc",
    "requirements-dev.lock": "pip-requirements",
    "requirements.lock": "pip-requirements",
    "tsconfig.*.json": "jsonc",
    "tsconfig.json": "jsonc",
    "*.js.jinja": "jinja",
    "*.snap": "markdown"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "mdx"
  ],
  "stylelint.validate": ["css", "postcss", "typescript", "typescriptreact"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "black-formatter.args": ["--config", "pyproject.toml"],
  "python.analysis.diagnosticMode": "workspace",
  "python.testing.pytestArgs": ["pyprojects"],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "rust-analyzer.cargo.buildScripts.enable": true,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.cargo.extraEnv": {
    "PYO3_PYTHON": "${workspaceFolder}/.venv/bin/python3"
  },
  "rust-analyzer.runnables.extraEnv": {
    "PYO3_PYTHON": "${workspaceFolder}/.venv/bin/python3"
  },
  "cSpell.allowCompoundWords": true,
  "cSpell.enabled": true,
  "cSpell.words": [
    "aiofiles",
    "algoliasearch",
    "astd",
    "awatch",
    "bufbuild",
    "dbaeumer",
    "dumi",
    "dumirc",
    "elems",
    "fdpexpect",
    "fdspawn",
    "hlistcol",
    "Idents",
    "kuscia",
    "lxml",
    "mdast",
    "mdclient",
    "mdserver",
    "mdxjs",
    "MDXJSESM",
    "npmjs",
    "nrwl",
    "pbar",
    "pbars",
    "proto",
    "protobuf",
    "protoc",
    "pyfunction",
    "pymodule",
    "pypa",
    "pypi",
    "pyproject",
    "pyright",
    "remarkrc",
    "scql",
    "tablefmt",
    "tgroup",
    "thead",
    "tqdm",
    "unist",
    "Vercel",
    "vitest",
    "webp",
    "yuque"
  ]
}
