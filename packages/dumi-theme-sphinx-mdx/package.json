    "@secretflow/dumi-plugin-search": "workspace:^",
    "antd": "^5.7.0",
    "graphviz-react": "^1.2.5",
    "katex": "^0.16.8",
    "mermaid": "^10.2.4",
    "styled-components": "^5.3.11",
    "yaml": "^2.3.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/styled-components": "^5.1.26",
    "@umijs/renderer-react": "^4.0.72",
    "antd": "^5.5.0",
    "dumi": "^2.2.1",
    "globby": "^13.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup-utils": "workspace:^"
  },
  "peerDependencies": {
    "dumi": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
