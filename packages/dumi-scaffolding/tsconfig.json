{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@@/*": [".dumi/tmp/*"]
    }
  },
  "include": [".dumi/**/*", "plugins/**/*.ts", ".dumirc.ts"]
}
