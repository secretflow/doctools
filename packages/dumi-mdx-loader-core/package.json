{
  "name": "@secretflow/dumi-mdx-loader-core",
  "license": "MIT",
  "version": "0.0.1",
  "description": "Use proper .mdx files in Dumi.",
  "keywords": [],
  "repository": "https://github.com/secretflow/doctools/tree/main/packages/dumi-mdx-loader-core",
  "bugs": "https://github.com/secretflow/doctools/issues",
  "author": "Tony Wu <tonywu6@protonmail.com>",
  "files": [
    "dist",
    "src"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "sideEffects": false,
  "module": "./dist/index.mjs",
  "types": "./dist/typing/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typing/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "setup": "tsup",
    "build": "tsup",
    "dev": "tsup --watch",
    "test:vitest": "vitest run",
    "lint:eslint": "eslint src",
    "lint:tsc": "tsc --noEmit",
    "release": "pnpm publish -r --filter ."
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@swc/core": "^1.3.69",
    "globby": "^13.2.2",
    "pluralize": "^8.0.0",
    "swc-loader": "^0.2.3"
  },
  "devDependencies": {
    "tsup-utils": "workspace:^",
    "@types/node": "^18.16.19",
    "@types/pluralize": "^0.0.29",
    "@umijs/bundler-webpack": "^4.0.72",
    "dumi": "^2.2.1"
  },
  "peerDependencies": {
    "dumi": "^2.2.1"
  }
}
